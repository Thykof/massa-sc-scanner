{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "install": "npm run build",
    "prebuild": "rimraf dist",
    "build": "./esbuild.js",
    "postbuild": "cd dist && zip -r scanner.zip scanner.js* && zip -r verifier.zip verifier.js*",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "serverless:dev": "nodemon -e ts -x \"npm run build && npx serverless offline\" -w ./src",
    "serverless": "npx serverless offline",
    "deploy": "npx serverless deploy",
    "undeploy": "npx serverless remove",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@massalabs/massa-sc-compiler": "^0.1.0",
    "@massalabs/massa-web3": "^4.0.2-dev",
    "adm-zip": "^0.5.12",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.2",
    "child_process": "^1.0.2",
    "lambda-multipart-parser": "^1.0.1",
    "typeorm": "^0.3.20",
    "wasmparser": "^5.11.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/aws-lambda": "^8.10.138",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.21.4",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.1.2",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.7",
    "serverless-offline": "^13.6.0",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
